{
  "target_column": "Target",
  "compute_feature_importance": true,  
  "preprocessing_steps": [
    {
      "name": "SimpleImputer",
      "params": {
        "use_scaler": 1
      }
    },
    {
      "name": "TransformRange",
      "params": {
        "columns_bin_sizes": {"Age": 10, "Income": 30000}  
      }
    },
    {
      "name": "DataScaler",
      "params": {
        "n": 0  
      }
    },
    {
      "name": "DataFrameEncoder",
      "params": {
        "norm_method": "median"  
      }
    },
    {
      "name": "remove_collinearity",
      "params": {
        "threshold": 1.0  
      }
    },
    {
      "name": "OneHotEncoder",
      "params": {
        "columns": ["Income_range"]  
      }
    },
    {
      "name": "OneHotEncoder",
      "params": {
        "columns": ["Age_range"]  
      }
    }
  ]
}
